{
  "name": "catalog",
  "version": "0.0.0",
  "scripts": {
    "build-patch":"cd ./projects/dp-catalog && npm version patch && cd .. && ng build --prod",
    "build-minor":"cd ./projects/dp-catalog && npm version minor && cd .. && ng build --prod",
    "build-major":"cd ./projects/dp-catalog && npm version major && cd .. && ng build --prod",
    "publish":"cd ./dist/dp-catalog && npm publish --registry http://localhost:4873",
    "deploy-patch": "npm run build-patch && npm run publish",
    "deploy-minor": "npm run build-minor && npm run publish",
    "deploy-major": "npm run build-major && npm run publish"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1",
    "@angular/common": "^9.1",
    "@angular/compiler": "^9.1",
    "@angular/core": "^9.1",
    "@angular/forms": "^9.1",
    "@angular/platform-browser": "^9.1",
    "@angular/platform-browser-dynamic": "^9.1",
    "@angular/router": "^9.1",
    "@bloomreach/ng-sdk": "^14.2",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.7",
    "@angular-devkit/build-ng-packagr": "~0.900.7",
    "@angular/cli": "^9.1",
    "@angular/compiler-cli": "^9.1",
    "@angular/language-service": "^9.1",
    "@types/node": "^12.11.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^9.0.0",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "typescript": "~3.8"
  }
}
